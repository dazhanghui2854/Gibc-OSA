/* 这些信息通过HWID解析得到，但其定义与HWID独立，利于产品维护。*/
typedef struct
{
    /* 产品线号，见定义SYS_ProductLineType。*/
    Uint16 nPrdtLineId;

    /* 产品型号，见定义SYS_ProductTypeXXX。 */
    Uint16 nPrdModelId;

    /* 硬件版本号，见定义SYS_HwVersionType。*/
    Uint16 nHwVersionId;

    /* 特殊需求号，见定义SYS_SpecNeedsType。*/
    Uint16 nSpecNeedsId;

    /* Sensor类型，见SYS_SensorType定义。*/
    Uint16 nSensorId;

    /* Sensor实例，用于两个相同的Sensor实例运行的情况。*/
    Uint16 nSensorInst;

    /* SOC中各MCU的频率，单位是MHz，索引定义见SYS_McuType。*/
    Uint16 nMcuRate[SYS_PROC_NR];

    /* Fpga类型，见SYS_FpgaType定义。*/
    Uint16 nFpgaId;

    /* DSP类型，见SYS_DspChip定义。*/
    Uint16 nDspChipId;

    /*加密类型，见SYS_Encrypt定义*/
    Uint16 nEncryptFlag;

    Uint16 reserve;
    Uint32 reserved[4];
} SYS_ProductInfo;


/* 共享内存区域 */
typedef enum
{
    /* 核间通信用内存，未开启Cache。*/
    SYS_SR_NON_CACHED      = 0,

    /* 核间通信用内存，开启了Cache，一般用于码流存储。*/
    SYS_SR_CACHED_BITSTREM = 1,

    /* Host处理器进程间通信专用，未开启Cache。*/
    SYS_SR_HOST_CACHED     = 2,

    /* 核间通信用内存，未开启Cache，一般用于视频数据存储。*/
    SYS_SR_VIDEO_FRAME     = 3,

    SYS_SR_NR              = 4
} SYS_SharedRegionId;


/* 时间结构体 */
typedef struct
{
    Int32L tvSec;     /* 秒 */
    Int32L tvUsec;    /* 微秒 */
} SYS_Timeval;


/* Buffer描述符，多模块间使用。*/
typedef struct
{
    Uint32  nIndex;        /* Buffer的ID, 获取者不可修改。*/
    Uint32  nBufSize;
    Uint32  nBytesUsed;
    Uint32  dataFormat;
    Uint32  flags;
    Uint32  timeStamp;
    Uint32  nWidth;
    Uint32  nHeight;
    Ptr     physAddr;
    Ptr     virtAddr;

    Uint32  reserved[4];
} SYS_BufInfo;


